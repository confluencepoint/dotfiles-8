{
  "*": {
    "path": ["**"],
    "suffixesadd": ".vim,.fish,.edn,.yml,.el,.itermcolors,.clj,.lua,.json,.conf,.md"
  },
  "README.md": {"type": "readme"},
  "nvim/*.vim": {"type": "vim"},
  "fish/*": {"type": "fish"},
  "tmux/*.conf": {"type": "tmux"},
  "macosx/*": {"type": "macosx"},
  "git/*": {"type": "git"},
  "emacs/*.el": {"type": "emacs"}
}
